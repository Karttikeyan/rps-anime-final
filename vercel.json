{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "remix",
  "installCommand": "npm ci --legacy-peer-deps",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/.well-known/farcaster.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
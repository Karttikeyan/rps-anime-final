{
  "version": 2,
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.well-known/farcaster.json",
      "dest": "/dist/.well-known/farcaster.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(.*)", 
      "dest": "/dist/index.html"
    }
  ]
}
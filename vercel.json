{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.well-known/farcaster.json",
      "dest": "/api/farcaster-manifest.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
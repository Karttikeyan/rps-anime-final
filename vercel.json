{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "framework": "remix",
  "installCommand": "npm ci --legacy-peer-deps",
  "routes": [
    {
      "src": "/.well-known/farcaster.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}